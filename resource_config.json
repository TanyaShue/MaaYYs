            "type": "boole",
            "default": false,
            "pipeline_override": {
                "探索_悬赏_自动轮换": {
                    "enabled": "{boole}"
                }
            }
        },
        {
            "name": "悬赏封印分组预设",
            "type": "input",
            "default": "默认分组",
            "pipeline_override": {
