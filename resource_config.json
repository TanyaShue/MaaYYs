        {
            "task_name": "结界奖励领取",
            "task_entry": "阴阳寮奖励领取",
            "option": [
                "寄养设置"
            ]
        }
    ],
    "options": [
        {
            "name": "寄养设置",
            "type": "select",
            "default": "勾玉优先",
            "pipeline_override": {
                "体力优先": {
                    "阴阳寮奖励领取_结界奖励领取_寄养式神_选择最优寄养": {
                        "custom_action_param": {
                            "FosterTarget": 2
                        }
                    }
                },
                "勾玉优先": {
                    "阴阳寮奖励领取_结界奖励领取_寄养式神_选择最优寄养": {
                        "custom_action_param": {
                            "FosterTarget": 1
                        }
                    }
                }
            },
            "choices": [
                {
                    "name": "勾玉优先",
                    "value": "勾玉优先"
                },
                {
                    "name": "体力优先",
                    "value": "体力优先"
                }
            ]
        },
        {
            "name": "选择区服",
            "type": "select",
            "default": "官服",
            "pipeline_override": {
                "官服": {},
                "官网下载版": {
                    "打开阴阳师": {
                        "package": "com.netease.onmyoji/.tag0"
                    }
                }
            },
            "choices": [
                {
                    "name": "官服",
                    "value": "官服"
                },
                {
                    "name": "官网下载版",
                    "value": "官网下载版"
                }
            ]
        },
