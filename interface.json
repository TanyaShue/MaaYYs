  "option": {
    "寄养设置": {
      "type": "select",
      "label": "寄养设置",
      "description": "选择自动寄养时优先考虑的收益类型，是优先体力还是勾玉",
      "default_case": "勾玉优先",
      "cases": [
        {
          "name": "勾玉优先",
          "label": "勾玉优先",
          "pipeline_override": {
            "阴阳寮奖励领取_结界奖励领取_寄养式神_选择最优寄养": {
              "custom_action_param": {
                "FosterTarget": 1
              }
            }
          }
        },
        {
          "name": "体力优先",
          "label": "体力优先",
          "pipeline_override": {
            "阴阳寮奖励领取_结界奖励领取_寄养式神_选择最优寄养": {
              "custom_action_param": {
                "FosterTarget": 2
              }
            }
          }
        }
      ]
    },
    "捐材料": {
      "type": "select",
      "label": "捐材料",
      "default_case": "低级觉醒材料",
      "cases": [
        {
          "name": "低级觉醒材料",
          "label": "低级觉醒材料",
          "pipeline_override": {
            "寮30_提交材料": {
              "expected": "提交低级觉醒材料"
            }
          }
        },
        {
          "name": "中级觉醒材料",
          "label": "中级觉醒材料",
          "pipeline_override": {
            "寮30_提交材料": {
              "expected": "提交中级觉醒材料"
            }
          }
        },
        {
          "name": "高级觉醒材料",
          "label": "高级觉醒材料",
          "pipeline_override": {
            "寮30_提交材料": {
              "expected": "提交高级觉醒材料"
            }
          }
        }
      ]
    },
    "地鬼挑战次数": {
      "type": "select",
      "label": "地鬼挑战次数",
      "default_case": "1个",
      "cases": [
        {
          "name": "1个",
          "label": "1个",
          "value": "1",
          "pipeline_override": {
            "开始自动挑战鬼王": {
              "custom_action_param": {
                "count": 1
              }
