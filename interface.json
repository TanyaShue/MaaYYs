      "cases": [
        {
          "name": "探索",
          "label": "只打探索",
          "value": "探索",
          "pipeline_override": {
            "悬赏封印2": {
              "enable": false
            }
          }
        },
        {
          "name": "挑战",
          "label": "挑战优先",
          "value": "挑战",
          "pipeline_override": {
            "悬赏封印2": {
              "enable": true
            }
          }
        }
      ]
    },
    "是否启用自动挂卡": {
      "type": "switch",
      "label": "是否启用自动挂卡",
      "description": "是否启用自动挂卡",
      "cases": [
        {
          "name": "Yes",
          "label": "是"
        },
        {
          "name": "No",
          "label": "否"
        }
      ],
      "pipeline_override": {
        "阴阳寮奖励领取_识别结界特征": {
          "custom_action_param": {
            "tasks": {
              "阴阳寮奖励24_挂结界卡": {
                "enabled": true
              }
