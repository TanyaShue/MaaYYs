        {
          "name": "3个",
          "label": "3个",
          "value": "3",
          "pipeline_override": {
            "开始自动挑战鬼王": {
              "custom_action_param": {
                "count": 3
              }
            }
          }
        }
      ]
    },
    "御魂挑战目标": {
      "type": "select",
      "label": "御魂挑战目标",
      "description": "根据你当前的地鬼分数选择需要挑战的地鬼个数",
      "default_case": "魂土",
      "cases": [
        {
          "name": "魂土",
          "label": "魂土",
          "value": "魂土",
          "pipeline_override": {
            "自动御魂_点击目标层数": {
              "expected": "悲鸣"
            }
          }
        },
        {
          "name": "魂十",
          "label": "魂十",
          "value": "魂十",
          "pipeline_override": {
            "自动御魂_点击目标层数": {
              "expected": "拾层"
            }
